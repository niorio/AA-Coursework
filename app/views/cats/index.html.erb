<h1>All Cats</h1>

<ul>
  <% @cats.each do |cat| %>
  <li>
    <a href="<%=cat_url(cat.id)%>"><%= cat.name %></a>
    <% if current_user == cat.owner %>
      <a href="<%=edit_cat_url(cat.id)%>">Edit</a>
    <% end %>
  </li>
  <% end %>
</ul>
<a href="<%= new_cat_rental_request_url%>">Rent a Cat</a>
<br>
<a href="<%= new_cat_url%>">Create Cat</a>
